{
    "name": "akaunting/laravel-language",
    "description": "Language switcher package for Laravel",
    "keywords": [
        "laravel",
        "language",
        "switcher"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Denis Duli√ßi",
            "email": "info@akaunting.com",
            "homepage": "https://akaunting.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jocelinkisenga/agent"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "jenssegers/agent": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Akaunting\\Language\\": "./src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Akaunting\\Language\\Provider"
            ],
            "aliases": {
                "Language": "Akaunting\\Language\\Facade"
            }
        }
    }
}
